syntax = "v1"

info (
	title:   "type title here"
	desc:    "type desc here"
	author:  "type author here"
	email:   "type email here"
	version: "type version here"
)

type GetRequest {
	Id uint64 `json:"id"`
}

type Account {
	Id   uint64 `json:"id"`
	Code string `json:"code"`
}

service user {
	@handler getAccount
	get /get (GetRequest) returns (Account)

	@handler Test
	get /test returns (Account)
}

